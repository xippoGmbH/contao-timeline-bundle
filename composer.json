{
    "name": "xippogmbh/contao-timeline-bundle",
    "type": "contao-bundle",
    "description": "Timeline Bundle for Contao 4",
    "homepage": "https://www.xippo.ch",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Xippo GmbH",
            "homepage": "https://github.com/xippogmbh"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.10.*",
        "symfony/framework-bundle": "^4"
    },
	"prefer-stable": true,
    "minimum-stability": "dev",
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
		"phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "XippoGmbH\\ContaoTimelineBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "XippoGmbH\\ContaoTimelineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XippoGmbH\\ContaoTimelineBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/xippogmbh/contao-timeline-bundle/issues",
        "source": "https://github.com/xippogmbh/contao-timeline-bundle"
    }
}
